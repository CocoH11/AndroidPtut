        android:layout_marginTop="8dp"
        android:text="@string/string_ville"
        app:layout_constraintStart_toEndOf="@+id/textViewDetailClientCodePostal"
        app:layout_constraintTop_toBottomOf="@+id/textViewDetailAdresseClient" />

    <TextView
        android:id="@+id/textViewDetailClientVille"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/defaultTextViewName"
        app:layout_constraintEnd_toEndOf="parent"
