        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/defaultTextViewName"
        app:layout_constraintEnd_toEndOf="parent"
