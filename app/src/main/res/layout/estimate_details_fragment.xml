        app:layout_constraintTop_toBottomOf="@+id/textViewDateDevis" />

    <TextView
        android:id="@+id/textViewDetailDevisIdClient"
        android:layout_width="285dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/defaultTextViewName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewNumIdClient"
        app:layout_constraintTop_toBottomOf="@+id/textViewDetailDevisDate" />

</androidx.constraintlayout.widget.ConstraintLayout>